package question02;

import java.util.Scanner;

public class Fibonacci2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = 1;
		int b = 0;
		int aux;
		System.out.print("Digite um número para saber se esse número faz parte da sequencia de Fibonacci: ");
		int n = sc.nextInt();
		Fibonacci fibo = new Fibonacci(n);
		for (int i=0;i<n;i++) {
			System.out.print(a);
			aux = a;
			a = a + b;
			b = aux;
		}
		System.out.println();
		System.out.println(fibo.fibo(n));
		sc.close();
	}

}
